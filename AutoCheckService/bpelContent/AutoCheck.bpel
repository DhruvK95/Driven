						</tns:AutoCheckResponse>
					</bpel:literal>
				</bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            
        </bpel:assign>
         <bpel:sequence><bpel:assign validate="no" name="AssignDLRequest">
		            <bpel:copy>
		                <bpel:from><bpel:literal><tns:validateDriversLicenseRequest xmlns:tns="http://exactmatch.assign2.soacourse.unsw.edu.au" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
