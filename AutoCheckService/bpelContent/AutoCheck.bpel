                    </bpel:assign>
                
            </bpel:sequence>
        </bpel:if>
        
        
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:AutoCheck"
               operation="validate" 
               variable="output"
               />
        
        
    </bpel:sequence>
</bpel:process>
