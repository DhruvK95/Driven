                        </bpel:to>
                    </bpel:copy>
                    <bpel:copy><bpel:from part="parameters" variable="ExactMatchServiceLinkResponse">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[messageStatus]]></bpel:query>
                        </bpel:from>
                        <bpel:to part="payload" variable="output">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:messageStatus]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>        
                </bpel:assign>
            
            </bpel:sequence>
            
            
           
     		<bpel:if name="IfDriversLicense">
        	<bpel:condition>bpel:getVariableData('output', 'payload', 'tns:messageStatus')='no'</bpel:condition>
            <bpel:sequence><bpel:assign validate="no" name="AssignAdrRequest"><bpel:copy>
			                <bpel:from>
			                    <bpel:literal>
			                        <tns:validateAddressRequest xmlns:tns="http://similarmatchservice.assign2.soacourse.unsw.edu.au" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
