						</bpel:literal></bpel:from>
		                <bpel:to variable="ExactMatchServiceLinkRequest" part="parameters"></bpel:to>
		            </bpel:copy>
		            
		            <bpel:copy>
		                <bpel:from part="payload" variable="input">
		                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:address]]></bpel:query>
		                </bpel:from>
		                <bpel:to part="parameters" variable="ExactMatchServiceLinkRequest">
		                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[address]]></bpel:query>
		                </bpel:to>
		            </bpel:copy>
	        	</bpel:assign>
                <bpel:invoke name="InvokeDL" partnerLink="ExactMatchServiceLink" operation="validateExactMatch" inputVariable="ExactMatchServiceLinkRequest" outputVariable="ExactMatchServiceLinkResponse" />
                
                <bpel:assign validate="no" name="AssignOutput">
                <bpel:copy><bpel:from part="parameters" variable="ExactMatchServiceLinkResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[message]]></bpel:query>
                        </bpel:from>
