			                        </tns:validateAddressRequest>
			                    </bpel:literal>
			                </bpel:from><bpel:to variable="SimilarMatchServiceLinkRequest" part="parameters"></bpel:to>
			            </bpel:copy>
			            <bpel:copy>
			                <bpel:from part="payload" variable="output">
			                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:message]]></bpel:query>
			                </bpel:from>
			                <bpel:to part="parameters" variable="SimilarMatchServiceLinkRequest">
			                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[address]]></bpel:query>
			                </bpel:to>
			                
			            </bpel:copy>
			        </bpel:assign>
        
                    <bpel:invoke name="InvokeADR" partnerLink="SimilarMatchLink" operation="validateSimilarMatch" portType="ns0:SimilarMatchService" inputVariable="SimilarMatchServiceLinkRequest" outputVariable="SimilarMatchServiceLinkResponse" />
                    
                    <bpel:assign validate="no" name="AssignOutput">
                    	<bpel:copy>
                            <bpel:from part="parameters" variable="SimilarMatchServiceLinkResponse">
                                <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[message]]></bpel:query>
                            </bpel:from>
                            <bpel:to part="payload" variable="output">
                                <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:message]]></bpel:query>
                            </bpel:to>
                        </bpel:copy>
                        <bpel:copy>
                            <bpel:from part="parameters" variable="SimilarMatchServiceLinkResponse">
                                <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[messageStatus]]></bpel:query>
                            </bpel:from>
